[
  {										// プリセット1
    "PresetName": "プリセット1",			// プリセット名 <--- プリセットを識別するための名前
    "TargetFileKeyword": "KeywordA",		// 対象ファイルキーワード <--- このキーワードがあるファイルのみ検査対象として取り込む
    "InspectionMethods": [					// 検査方法
      {											// 検査方法1 <--- SheetAのA1セルがOKかどうか検査
        "SheetName": "SheetA",						// シート名
        "Cell": "A1",								// セル
        "Condition": "Equal",						// 条件
        "Value": "OK"								// 値
      },
      {											// 検査方法2 <--- SheetAのA2セルが空ではないか検査
        "SheetName": "SheetA",						// シート名
        "Cell": "A2",								// セル
        "Condition": "NotEmpty",					// 条件
        "Value": "―"								// 値
      }
    ]
  },
  {										// プリセット2
    "PresetName": "プリセット2",			// プリセット名
    "TargetFileKeyword": "KeywordB",		// 対象ファイルキーワード
    "InspectionMethods": [					// 検査方法
      {											// 検査方法1
        "SheetName": "SheetB",						// シート名
        "Cell": "B1",								// セル
        "Condition": "Equal",						// 条件
        "Value": "OK"								// 値
      },
      {											// 検査方法2
        "SheetName": "SheetB",						// シート名
        "Cell": "B2",								// セル
        "Condition": "NotEmpty",					// 条件
        "Value": "―"								// 値
      }
    ]
  }
]
